{
  "name": "browser-dtector",
  "version": "4.1.0",
  "description": "A Javascript library to detect browser, version and platform",
  "author": "sibiraj-s",
  "license": "MIT",
  "type": "module",
  "repository": "github:sibiraj-s/browser-dtector",
  "homepage": "https://github.com/sibiraj-s/browser-dtector#readme",
  "bugs": "https://github.com/sibiraj-s/browser-dtector/issues",
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/sibiraj-s"
    }
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "./dist/browser-dtector.cjs",
  "module": "./dist/browser-dtector.mjs",
  "types": "./dist/browser-dtector.d.mts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/browser-dtector.d.cts",
        "default": "./dist/browser-dtector.cjs"
      },
      "import": {
        "types": "./dist/browser-dtector.d.mts",
        "default": "./dist/browser-dtector.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "build:docs": "vite build --base /browser-dtector/",
    "dev": "vite",
    "test": "vitest",
    "lint": "oxlint .",
    "prepare": "is-ci || husky"
  },
  "keywords": [
    "browser-detector",
    "detect-browser",
    "useragent-parser",
    "ua-parser"
  ],
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@fontsource-variable/inconsolata": "^5.2.8",
    "@tailwindcss/vite": "^4.1.17",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^16.2.7",
    "oxlint": "^1.30.0",
    "postcss": "^8.5.6",
    "publint": "^0.3.15",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "tailwindcss": "^4.1.17",
    "tsdown": "^0.17.0-beta.4",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.14"
  }
}
